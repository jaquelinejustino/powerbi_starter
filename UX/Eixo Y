Eixo Y din칙mico = 
VAR vTabela = 
	ALLSELECTED(dCalendario[Data])

VAR vMaior_valor = 
	MAXX(
		vTabela, 
		[medida
  )

VAR vdeslocamento = 
	1.2 -- Valor ajust치vel

return
	vMaior_valor + vdeslocamento

*******************************************************************

Eixo Y Din칙mico = 
CALCULATE(
    MAXX(ALL(dCalendario[Data]), [medida]
    ),
    ALL(dCalendario)
) * 1.2 -- Valor ajust치vel
